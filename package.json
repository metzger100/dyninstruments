{
  "name": "dyninstruments",
  "version": "0.0.0-test",
  "private": true,
  "description": "Dev-only tooling for dyninstruments tests",
  "scripts": {
    "ai:sync:agents": "node tools/sync-ai-instructions.mjs --from=agents",
    "ai:sync:claude": "node tools/sync-ai-instructions.mjs --from=claude",
    "ai:check": "node tools/check-ai-instructions.mjs",
    "docs:check": "node tools/check-docs.mjs && node tools/check-doc-format.mjs --warn && npm run check:reachability && npm run ai:check",
    "check:reachability": "node tools/check-doc-reachability.mjs",
    "check:docformat": "node tools/check-doc-format.mjs",
    "check:filesize": "node tools/check-file-size.mjs",
    "check:headers": "node tools/check-headers.mjs",
    "check:deps": "node tools/check-dependencies.mjs",
    "check:umd": "node tools/check-umd.mjs",
    "check:naming": "node tools/check-naming.mjs",
    "check:patterns": "node tools/check-patterns.mjs",
    "check:smells": "node tools/check-patterns.mjs && node tools/check-smell-contracts.mjs",
    "gc:status": "node tools/gc-baseline.mjs --status",
    "gc:update-baseline": "node tools/gc-baseline.mjs --update-head",
    "hooks:install": "node tools/install-hooks.mjs",
    "hooks:doctor": "node tools/check-hooks.mjs",
    "check:all": "npm run check:smells && node tools/check-docs.mjs && node tools/check-doc-format.mjs --warn && node tools/check-doc-reachability.mjs && npm run ai:check && node tools/check-file-size.mjs && node tools/check-headers.mjs && node tools/check-dependencies.mjs && node tools/check-umd.mjs && node tools/check-naming.mjs",
    "check:strict": "npm run check:all && npm run test:coverage:check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "npm run test:coverage && node tools/check-coverage.mjs"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^26.0.0",
    "vitest": "^2.1.8"
  }
}
