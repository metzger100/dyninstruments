{
  "name": "dyninstruments",
  "version": "0.0.0-test",
  "private": true,
  "description": "Dev-only tooling for dyninstruments tests",
  "scripts": {
    "ai:sync:agents": "node tools/sync-ai-instructions.mjs --from=agents",
    "ai:sync:claude": "node tools/sync-ai-instructions.mjs --from=claude",
    "ai:check": "node tools/check-ai-instructions.mjs",
    "docs:check": "node tools/check-docs.mjs && npm run ai:check",
    "check:filesize": "node tools/check-file-size.mjs",
    "check:headers": "node tools/check-headers.mjs",
    "check:naming": "node tools/check-naming.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "npm run test:coverage && node tools/check-coverage.mjs"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^26.0.0",
    "vitest": "^2.1.8"
  }
}
